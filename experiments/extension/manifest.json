{
  "manifest_version": 3,
  "name": "fasdfs Background Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}